---
import PageLayout from '../layouts/PageLayout.astro';
const pageTitle = "Thank you for your inquiry";
const description = "Thank you for your inquiry. We will review the content and get back to you shortly, so please wait for a while.";
---

<PageLayout pageTitle={pageTitle} description={description}>
<p>Thank you for your inquiry. We will review the content and get back to you shortly, so please wait for a while.</p>
<p>Please note that we may not be able to respond depending on the content.</p>
<p>Thank you for your continued support of the naomina121 site.</p>
<a href="/"
  class="inline-block mt-6 px-6 py-3 bg-gray-700 hover:bg-gray-600 text-white transition-colors"
>Back to Home</a>
</PageLayout>
